<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Oracle Corporation//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>notify</servlet-name>
        <servlet-class>myapp.Notify</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>notify</servlet-name>
        <url-pattern>/notify</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>myapp.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login1</servlet-name>
        <servlet-class>myapp.login1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login1</servlet-name>
        <url-pattern>/login1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>displaystatus</servlet-name>
        <servlet-class>myapp.DisplayStatus</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>displaystatus</servlet-name>
        <url-pattern>/displaystatus</url-pattern>
    </servlet-mapping>


</web-app>
